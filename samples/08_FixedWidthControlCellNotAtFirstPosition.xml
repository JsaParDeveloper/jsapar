<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://jsapar.tigris.org/JSaParSchema/1.7" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/1.7 ../src/main/resources/xml/schema/JSaParSchema.xsd ">
  <fixedwidthschema lineseparator="&#13;&#10;">
    <line linetype="Line" trimfillcharacters="true">
      <cell name="Prefix" length="4"></cell>
      <recordcell name="Content" length="26">
        <fixedwithcontrolcellrecord>
          <controlcell length="1" />
          <record recordtype="Header" recordtypecontrolvalue="H">
            <cell name="FileName" length="15" />
            <cell name="Created date" length="10" />
          </record>
          <record recordtype="Person" recordtypecontrolvalue="B">
            <cell name="First name" length="8" />
            <cell name="Middle name" length="9" ignoreread="true" />
            <cell name="Last name" length="8" />
          </record>
          <record recordtype="Footer" recordtypecontrolvalue="F">
            <cell name="Rowcount" length="7" />
            <cell name="Filler" length="18" ignoreread="true" />
          </record>
        </fixedwithcontrolcellrecord>
      </recordcell>
    </line>
  </fixedwidthschema>
</schema>
